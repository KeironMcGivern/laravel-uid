{
  "name": "ollico/laravel-uid",
  "description": "A handy package to generate unique identifiers for Eloquent models",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "hashids/hashids": "^4.0",
    "illuminate/database": "^8.0|^9.0",
    "illuminate/container": "^8.0|^9.0",
    "illuminate/support": "^8.0|^9.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0"
  },
  "autoload": {
    "psr-4": {
      "Ollico\\Uid\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ollico\\Uid\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Ollico\\Uid\\UidServiceProvider"
      ]
    }
  }
}
